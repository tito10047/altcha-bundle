{
  "name": "tito10047/altcha-bundle",
  "type": "symfony-bundle",
  "description": "A simple package to help integrate Altcha on Symfony.",
  "keywords": [
    "symfony bundle",
    "altcha",
    "form security",
    "captcha",
    "form",
    "bundle",
    "symfony",
    "symfony-ux"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Hugo Posnic",
      "homepage": "https://github.com/Huluti",
      "role": "Founder of package"
    },
    {
      "name": "Jozef MÃ´stka",
      "homepage": "https://vsetkosada.sk/en",
      "email": "kontakt@vsetkosada.sk",
      "role": "Main developer of package"
    }
  ],
  "require": {
    "php": ">=8.1",
    "altcha-org/altcha": "^1.1.1",
    "symfony/console": "^6.4|^7.4|8.0",
    "symfony/form": "^6.4|^7.4|8.0",
    "symfony/framework-bundle": "^6.4|^7.4|8.0",
    "symfony/http-client": "^6.4|^7.4|8.0",
    "symfony/translation": "^6.4|^7.4|8.0",
    "symfony/twig-bundle": "^6.4|^7.4|8.0",
    "symfony/validator": "^6.4|^7.4|8.0",
    "symfony/yaml": "^6.4|^7.4|8.0"
  },
  "autoload": {
    "psr-4": {
      "Tito10047\\AltchaBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tito10047\\AltchaBundle\\Tests\\": "tests/"

    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-symfony": "^1.2",
    "rector/rector": "^0.14.5",
    "phpunit/phpunit": "^10.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/dotenv": "^6.4|7.4.*|8.0",
    "symfony/form": "^6.4|7.4.*|8.0",
    "symfony/framework-bundle": "^6.4|7.4.*|8.0",
    "symfony/runtime": "^6.4|7.4.*|8.0",
    "symfony/stimulus-bundle": "^2.30",
    "symfony/validator": "^6.4|7.4.*|8.0",
    "symfony/yaml": "^6.4|7.4.*|8.0",
    "symfony/web-profiler-bundle": "^6.4|7.4.*|8.0",
    "symfony/debug-bundle": "^6.4|7.4.*|8.0",
    "symfony/webpack-encore-bundle": "^2.3",
    "twig/twig": "^2.12|^3.21.1",
    "symfony/panther": "^2.2",
    "dbrekelmans/bdi": "^1.4",
    "symfony/asset-mapper": "^6.4|^7.4|8.0"
  },
  "scripts" : {
    "test": [
      "phpunit"
    ]
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "extra": {
    "symfony-web-dir": "./tests/App/AssetMapper/public",
    "public-dir": "./tests/App/AssetMapper/public",
    "symfony": {
      "require": "8.0.*"
    }
  },
  "minimum-stability": "beta"
}
